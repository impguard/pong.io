{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": {
            "options": [
                "single",
                "avoid-escape"
            ]
        },
        "no-bitwise": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    }
}